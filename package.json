    "sass": "^1.54.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.1.7",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.10.3",
    "unplugin-element-plus": "^0.4.1",
