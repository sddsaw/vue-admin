    "pinia": "^2.0.17",
    "pinia-plugin-persistedstate": "^2.1.0",
    "screenfull": "^6.0.2",
    "vue": "^3.2.37",
    "vue-router": "4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@iconify/vue": "^3.2.1",
